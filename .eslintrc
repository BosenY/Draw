{
    "extends": "airbnb-base",
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    },
    "globals": {
      "window": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-param-reassign": 0,
      "no-new": 0
    }
  }